{
  "name": "qianduan-rizhi",
  "version": "1.3.2",
  "description": "front-end diary blog",
  "main": "index.js",
  "scripts": {
    "lint": "prettier --write ./docs/*/**.md && lint-md ./docs/blog",
    "lint:fix": "lint-md ./docs --fix",
    "dev": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" vuepress dev docs",
    "build": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" vuepress build docs",
    "commit": "pnpm run lint && npx git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint"
    }
  },
  "author": "mengsixing",
  "license": "ISC",
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.8.0",
    "@vuepress/plugin-medium-zoom": "^1.8.0",
    "@vuepress/plugin-pwa": "^1.8.0",
    "cross-env": "^7.0.3",
    "husky": "^4.3.6",
    "lint-md": "^0.2.0",
    "lint-md-cli": "^0.1.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "element-ui": "^2.15.1",
    "vuepress": "^1.8.0"
  }
}
